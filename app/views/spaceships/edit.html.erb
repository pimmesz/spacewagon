<div class="flexcontainer">
<div class="flexform">

<h1>Edit Spaceship</h1>
<%= simple_form_for @spaceship do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :captain %>
  <%= f.input :location %>
  <%= f.input :speed %>
  <%= f.input :capacity %>
  <%= f.input :price_per_day %>
  <%= f.button :submit %>
<% end %>

</div>
</div>
